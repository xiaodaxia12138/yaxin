(select user_id,sum(call_duration_m)zj_mou
     from default.dw_user_call_dt_yyyymmdd
      where day_id=20190831 and cdrtype_id=1
     group by user_id 
   )c on c.user_id = a.user_id
where c.zj_mou=0 or c.user_id is null;

